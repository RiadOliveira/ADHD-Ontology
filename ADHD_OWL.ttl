###  http://adhd.com#Patient
:Patient rdf:type owl:Class ;
         rdfs:subClassOf gufo:FunctionalComplex ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :satisfiesCriterion ;
                           owl:maxQualifiedCardinality "18"^^xsd:nonNegativeInteger ;
                           owl:onClass :DSM5ADHDDiagnosticCriteria
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :vulnerableTo ;
                           owl:maxQualifiedCardinality "7"^^xsd:nonNegativeInteger ;
                           owl:onClass :ADHDRiskFactor
                         ] .


###  http://adhd.com#PatientWithADHD
:PatientWithADHD rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( :ADHD
                                                            :Patient
                                                            [ rdf:type owl:Class ;
                                                              owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                              owl:onProperty :exhibits ;
                                                                              owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                                              owl:onClass :ChildADHDSignal
                                                                            ]
                                                                            [ rdf:type owl:Restriction ;
                                                                              owl:onProperty :exhibits ;
                                                                              owl:maxQualifiedCardinality "11"^^xsd:nonNegativeInteger ;
                                                                              owl:onClass :TeenAdultADHDSignal
                                                                            ]
                                                                          )
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :suffersFrom ;
                                                              owl:someValuesFrom :Dysfunction
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :consumes ;
                                                              owl:maxQualifiedCardinality "5"^^xsd:nonNegativeInteger ;
                                                              owl:onClass :Drugs
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :vulnerableTo ;
                                                              owl:maxQualifiedCardinality "7"^^xsd:nonNegativeInteger ;
                                                              owl:onClass :ADHDRiskFactor
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf :ADHD ,
                                 :Patient ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :suffersFrom ;
                                   owl:someValuesFrom :Dysfunction
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :satisfiesCriterion ;
                                   owl:minQualifiedCardinality "6"^^xsd:nonNegativeInteger ;
                                   owl:onClass :DSM5ADHDDiagnosticCriteria
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :consumes ;
                                   owl:maxQualifiedCardinality "5"^^xsd:nonNegativeInteger ;
                                   owl:onClass :Drugs
                                 ] .


###  http://adhd.com#Perception
:Perception rdf:type owl:Class ;
            rdfs:subClassOf :Dysfunction .


###  http://adhd.com#PersonalityDisordersAndAntisocialBehavior
:PersonalityDisordersAndAntisocialBehavior rdf:type owl:Class ;
                                           rdfs:subClassOf :TeenAdultADHDSignal .


###  http://adhd.com#Physiological
:Physiological rdf:type owl:Class ;
               rdfs:subClassOf :PotentialCause .


###  http://adhd.com#PotentialCause
:PotentialCause rdf:type owl:Class ;
                rdfs:subClassOf gufo:FunctionalComplex .


###  http://adhd.com#PredominantHyperactivityimpulsivity
:PredominantHyperactivityimpulsivity rdf:type owl:Class ;
                                     rdfs:subClassOf :ADHDClassification ,
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :definedBy ;
                                                       owl:someValuesFrom :DSM5ADHDHyperactivityimpulsivitySymptom
                                                     ] .


###  http://adhd.com#PredominantInattention
:PredominantInattention rdf:type owl:Class ;
                        rdfs:subClassOf :ADHDClassification ,
                                        [ rdf:type owl:Restriction ;
