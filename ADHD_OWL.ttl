@prefix : <http://adhd.com#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix gufo: <http://purl.org/nemo/gufo#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://adhd.com#> .

<http://adhd.com> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://adhd.com#amplifiedBy
:amplifiedBy rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :ChildADHDSignal ;
             rdfs:range :ADHDSignalIntensifier ;
             rdfs:label "amplifiedBy"@en .


###  http://adhd.com#attributedTo
:attributedTo rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :ADHD ;
              rdfs:range :PotentialCause .


###  http://adhd.com#characterizedBy
:characterizedBy rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain :ADHD ;
                 rdfs:range :DSM5ADHDDiagnosticCriteria ;
                 rdfs:label "characterizedBy"@en .


###  http://adhd.com#classifiedBy
:classifiedBy rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :ADHD ;
              rdfs:range :ADHDClassification .


###  http://adhd.com#composedOf
:composedOf rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :Combined ;
            rdfs:range [ owl:intersectionOf ( :PredominantHyperactivityimpulsivity
                                              :PredominantInattention
                                            ) ;
                         rdf:type owl:Class
                       ] .


###  http://adhd.com#comprises
:comprises rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain :DSM5ADHDDiagnosticCriteria ;
           rdfs:range :DSM5ADHDValidSymptomCriteria ;
           rdfs:label "comprises"@en .


###  http://adhd.com#consumes
:consumes rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:domain :PatientWithADHD ;
          rdfs:range :Drugs .


###  http://adhd.com#definedBy
:definedBy rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Combined
                                       :PredominantHyperactivityimpulsivity
                                       :PredominantInattention
                                     )
                       ] ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( :DSM5ADHDCombinedSymptom
                                      :DSM5ADHDHyperactivityimpulsivitySymptom
                                      :DSM5ADHDInattentionSymptom
                                    )
                      ] ;
           rdfs:label "definedBy"@en .


###  http://adhd.com#exhibits
:exhibits rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :ChildWithADHD
                                      :TeenAdultWithADHD
                                    )
                      ] ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :ChildADHDSignal
                                     :TeenAdultADHDSignal
                                   )
                     ] .


###  http://adhd.com#mediates
:mediates rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty .


###  http://adhd.com#satisfiesCriterion
:satisfiesCriterion rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdfs:domain :Patient ;
                    rdfs:range :DSM5ADHDDiagnosticCriteria .


###  http://adhd.com#suffersFrom
:suffersFrom rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :PatientWithADHD ;
             rdfs:range :Dysfunction .


###  http://adhd.com#vulnerableTo
:vulnerableTo rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :Patient ;
              rdfs:range :ADHDRiskFactor .


#################################################################
#    Data properties
#################################################################

###  http://adhd.com#ageDiagnosed
:ageDiagnosed rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain :PatientWithADHD ;
              rdfs:range xsd:integer .


###  http://adhd.com#diagnosisDate
:diagnosisDate rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain :PatientWithADHD ;
               rdfs:range xsd:dateTime .


###  http://adhd.com#timesThatSelfMedicated
:timesThatSelfMedicated rdf:type owl:DatatypeProperty ;
                        rdfs:subPropertyOf owl:topDataProperty ;
                        rdfs:domain :PatientWithADHD ;
                        rdfs:range xsd:integer .


###  http://adhd.com#timesThatTookDrugs
:timesThatTookDrugs rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf owl:topDataProperty ;
                    rdfs:domain :PatientWithADHD ;
                    rdfs:range xsd:integer .


###  http://adhd.com#treatmentStartDate
:treatmentStartDate rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf owl:topDataProperty ;
                    rdfs:domain :PatientWithADHD ;
                    rdfs:range xsd:dateTime .


#################################################################
#    Classes
#################################################################

###  http://adhd.com#ADHD
:ADHD rdf:type owl:Class ;
      rdfs:subClassOf gufo:FunctionalComplex ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :attributedTo ;
                        owl:someValuesFrom :PotentialCause
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :characterizedBy ;
                        owl:someValuesFrom :DSM5ADHDDiagnosticCriteria
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :classifiedBy ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :ADHDClassification
                      ] .


###  http://adhd.com#ADHDClassification
:ADHDClassification rdf:type owl:Class ;
                    rdfs:subClassOf gufo:FunctionalComplex .


###  http://adhd.com#ADHDRiskFactor
:ADHDRiskFactor rdf:type owl:Class ;
                rdfs:subClassOf gufo:Quality .


###  http://adhd.com#ADHDSignalIntensifier
:ADHDSignalIntensifier rdf:type owl:Class ;
                       rdfs:subClassOf gufo:FunctionalComplex .


###  http://adhd.com#AcademicActivities
:AcademicActivities rdf:type owl:Class ;
                    rdfs:subClassOf :ADHDSignalIntensifier ;
                    owl:disjointWith :PresenceInTraditionalClasses .


###  http://adhd.com#AcademicDeficiencies
:AcademicDeficiencies rdf:type owl:Class ;
                      rdfs:subClassOf :TeenAdultADHDSignal .


###  http://adhd.com#AdverseChildhoodExperiences
:AdverseChildhoodExperiences rdf:type owl:Class ;
                             rdfs:subClassOf :ADHDRiskFactor .


###  http://adhd.com#Aggressiveness
:Aggressiveness rdf:type owl:Class ;
                rdfs:subClassOf :TeenAdultADHDSignal .


###  http://adhd.com#Agitation
:Agitation rdf:type owl:Class ;
           rdfs:subClassOf :TeenAdultADHDSignal .


###  http://adhd.com#Alcohol
:Alcohol rdf:type owl:Class ;
         rdfs:subClassOf :FetalDrugExposure .


###  http://adhd.com#Amphetamines
:Amphetamines rdf:type owl:Class ;
              rdfs:subClassOf :Illegal .


###  http://adhd.com#Attention
:Attention rdf:type owl:Class ;
           rdfs:subClassOf :Dysfunction .


###  http://adhd.com#AvoidsOrDislikesTasksRequiringSustainedMentalEffort
:AvoidsOrDislikesTasksRequiringSustainedMentalEffort rdf:type owl:Class ;
                                                     rdfs:subClassOf :DSM5ADHDInattentionSymptom .


###  http://adhd.com#BeMorePronouncedThanExpected
:BeMorePronouncedThanExpected rdf:type owl:Class ;
                              rdfs:subClassOf :DSM5ADHDValidSymptomCriteria .


###  http://adhd.com#BePresentBeforeAge12
:BePresentBeforeAge12 rdf:type owl:Class ;
                      rdfs:subClassOf :DSM5ADHDValidSymptomCriteria .


###  http://adhd.com#BePresentOftenFor6MonthsOrMore
:BePresentOftenFor6MonthsOrMore rdf:type owl:Class ;
                                rdfs:subClassOf :DSM5ADHDValidSymptomCriteria .


###  http://adhd.com#Behavioral
:Behavioral rdf:type owl:Class ;
            rdfs:subClassOf :PotentialCause .


###  http://adhd.com#Biochemicals
:Biochemicals rdf:type owl:Class ;
              rdfs:subClassOf :PotentialCause .


###  http://adhd.com#Caffeine
:Caffeine rdf:type owl:Class ;
          rdfs:subClassOf :Legal ;
          owl:disjointWith :Nicotine .


###  http://adhd.com#Cannabis
:Cannabis rdf:type owl:Class ;
          rdfs:subClassOf :Illegal .


###  http://adhd.com#ChildADHDSignal
:ChildADHDSignal rdf:type owl:Class ;
                 rdfs:subClassOf gufo:Quality ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :amplifiedBy ;
                                   owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                   owl:onClass :ADHDSignalIntensifier
                                 ] .


###  http://adhd.com#ChildWithADHD
:ChildWithADHD rdf:type owl:Class ;
               rdfs:subClassOf :PatientWithADHD ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :exhibits ;
                                 owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                 owl:onClass :ChildADHDSignal
                               ] .


###  http://adhd.com#Cocaine
:Cocaine rdf:type owl:Class ;
         rdfs:subClassOf :Illegal .


###  http://adhd.com#Cocaine_1
:Cocaine_1 rdf:type owl:Class ;
           rdfs:subClassOf :FetalDrugExposure .


###  http://adhd.com#CoexistenceOfLowIntelligence
:CoexistenceOfLowIntelligence rdf:type owl:Class ;
                              rdfs:subClassOf :TeenAdultADHDSignal .


###  http://adhd.com#Combined
:Combined rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :PredominantHyperactivityimpulsivity
                                                     :PredominantInattention
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf :ADHDClassification ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :definedBy ;
                            owl:someValuesFrom :DSM5ADHDCombinedSymptom
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :composedOf ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :PredominantHyperactivityimpulsivity
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :composedOf ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :PredominantInattention
                          ] .


###  http://adhd.com#Communication
:Communication rdf:type owl:Class ;
               rdfs:subClassOf :Dysfunction .


###  http://adhd.com#DSM5ADHDCombinedSymptom
:DSM5ADHDCombinedSymptom rdf:type owl:Class ;
                         owl:equivalentClass [ owl:intersectionOf ( :DSM5ADHDHyperactivityimpulsivitySymptom
                                                                    :DSM5ADHDInattentionSymptom
                                                                  ) ;
                                               rdf:type owl:Class
                                             ] ;
                         rdfs:subClassOf :DSM5ADHDDiagnosticCriteria ,
                                         gufo:Relator ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :mediates ;
                                           owl:minQualifiedCardinality "6"^^xsd:nonNegativeInteger ;
                                           owl:onClass :DSM5ADHDHyperactivityimpulsivitySymptom
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :mediates ;
                                           owl:minQualifiedCardinality "6"^^xsd:nonNegativeInteger ;
                                           owl:onClass :DSM5ADHDInattentionSymptom
                                         ] .


###  http://adhd.com#DSM5ADHDDiagnosticCriteria
:DSM5ADHDDiagnosticCriteria rdf:type owl:Class ;
                            rdfs:subClassOf gufo:Quality ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty [ owl:inverseOf :satisfiesCriterion
                                                             ] ;
                                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                              owl:onClass :ChildWithADHD
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty [ owl:inverseOf :satisfiesCriterion
                                                             ] ;
                                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                              owl:onClass :TeenAdultWithADHD
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :comprises ;
                                              owl:qualifiedCardinality "5"^^xsd:nonNegativeInteger ;
                                              owl:onClass :DSM5ADHDValidSymptomCriteria
                                            ] .


###  http://adhd.com#DSM5ADHDHyperactivityimpulsivitySymptom
:DSM5ADHDHyperactivityimpulsivitySymptom rdf:type owl:Class ;
                                         rdfs:subClassOf :DSM5ADHDDiagnosticCriteria ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty [ owl:inverseOf :mediates
                                                                          ] ;
                                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass :DSM5ADHDCombinedSymptom
                                                         ] .


###  http://adhd.com#DSM5ADHDInattentionSymptom
:DSM5ADHDInattentionSymptom rdf:type owl:Class ;
                            rdfs:subClassOf :DSM5ADHDDiagnosticCriteria ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty [ owl:inverseOf :mediates
                                                             ] ;
                                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                              owl:onClass :DSM5ADHDCombinedSymptom
                                            ] .


###  http://adhd.com#DSM5ADHDValidSymptomCriteria
:DSM5ADHDValidSymptomCriteria rdf:type owl:Class ;
                              rdfs:subClassOf gufo:FunctionalComplex .


###  http://adhd.com#DeficientSocialSkills
:DeficientSocialSkills rdf:type owl:Class ;
                       rdfs:subClassOf :TeenAdultADHDSignal .


###  http://adhd.com#DifficultiesAssimilatingAppropriateSocialBehavior
:DifficultiesAssimilatingAppropriateSocialBehavior rdf:type owl:Class ;
                                                   rdfs:subClassOf :TeenAdultADHDSignal .


###  http://adhd.com#DifficultyMaintainingAttentionInTasksOrGames
:DifficultyMaintainingAttentionInTasksOrGames rdf:type owl:Class ;
                                              rdfs:subClassOf :DSM5ADHDInattentionSymptom .


###  http://adhd.com#DoesntSeemToListenWhenSpokenTo
:DoesntSeemToListenWhenSpokenTo rdf:type owl:Class ;
                                rdfs:subClassOf :DSM5ADHDInattentionSymptom .


###  http://adhd.com#Drugs
:Drugs rdf:type owl:Class ;
       rdfs:subClassOf gufo:FunctionalComplex ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty [ owl:inverseOf :mediates
                                        ] ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass :SelfMedication
                       ] .


###  http://adhd.com#Dysfunction
:Dysfunction rdf:type owl:Class ;
             rdfs:subClassOf gufo:Quality .


###  http://adhd.com#EasilyDistracted
:EasilyDistracted rdf:type owl:Class ;
                  rdfs:subClassOf :DSM5ADHDInattentionSymptom .


###  http://adhd.com#FailsToFollowInstructionsOrCompleteTasks
:FailsToFollowInstructionsOrCompleteTasks rdf:type owl:Class ;
                                          rdfs:subClassOf :DSM5ADHDInattentionSymptom .


###  http://adhd.com#FetalDrugExposure
:FetalDrugExposure rdf:type owl:Class ;
                   rdfs:subClassOf :ADHDRiskFactor .


###  http://adhd.com#ForgetfulInDailyActivities
:ForgetfulInDailyActivities rdf:type owl:Class ;
                            rdfs:subClassOf :DSM5ADHDInattentionSymptom .


###  http://adhd.com#FrequentlyAnswersQuestionsAbruptlyBeforeTheyreCompleted
:FrequentlyAnswersQuestionsAbruptlyBeforeTheyreCompleted rdf:type owl:Class ;
                                                         rdfs:subClassOf :DSM5ADHDHyperactivityimpulsivitySymptom .


###  http://adhd.com#FrequentlyFidgetsWithHandsOrFeet
:FrequentlyFidgetsWithHandsOrFeet rdf:type owl:Class ;
                                  rdfs:subClassOf :DSM5ADHDHyperactivityimpulsivitySymptom .


###  http://adhd.com#FrequentlyInterruptsOrIntrudesOnOthers
:FrequentlyInterruptsOrIntrudesOnOthers rdf:type owl:Class ;
                                        rdfs:subClassOf :DSM5ADHDHyperactivityimpulsivitySymptom .


###  http://adhd.com#GeneticFactors
:GeneticFactors rdf:type owl:Class ;
                rdfs:subClassOf :PotentialCause .


###  http://adhd.com#HasDifficultyPlayingQuietly
:HasDifficultyPlayingQuietly rdf:type owl:Class ;
                             rdfs:subClassOf :DSM5ADHDHyperactivityimpulsivitySymptom .


###  http://adhd.com#HeadTrauma
:HeadTrauma rdf:type owl:Class ;
            rdfs:subClassOf :ADHDRiskFactor .


###  http://adhd.com#Illegal
:Illegal rdf:type owl:Class ;
         rdfs:subClassOf :Drugs ;
         owl:disjointWith :Legal .


###  http://adhd.com#Impulsiveness
:Impulsiveness rdf:type owl:Class ;
               rdfs:subClassOf :TeenAdultADHDSignal .


###  http://adhd.com#InattentionOrFrequantMistakesInSchoolOrEverydayTasks
:InattentionOrFrequantMistakesInSchoolOrEverydayTasks rdf:type owl:Class ;
                                                      rdfs:subClassOf :DSM5ADHDInattentionSymptom .


###  http://adhd.com#InterfereWithFunctioningAtHomeSchoolOrWork
:InterfereWithFunctioningAtHomeSchoolOrWork rdf:type owl:Class ;
                                            rdfs:subClassOf :DSM5ADHDValidSymptomCriteria .


###  http://adhd.com#IronDeficiency
:IronDeficiency rdf:type owl:Class ;
                rdfs:subClassOf :ADHDRiskFactor .


###  http://adhd.com#LeadExposure
:LeadExposure rdf:type owl:Class ;
              rdfs:subClassOf :ADHDRiskFactor .


###  http://adhd.com#Legal
:Legal rdf:type owl:Class ;
       rdfs:subClassOf :Drugs .


